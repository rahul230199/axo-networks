LIMIT\04010;
SELECT\040id,\040user_email,\040company_name,\040factory_location,\040created_at\040FROM\040suppliers\040ORDER\040BY\040id\040DESC\040LIMIT\04010;
USE\040axo;
CREATE\040TABLE\040IF\040NOT\040EXISTS\040oems\040(
\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040user_email\040VARCHAR(255),
\040\040company_name\040VARCHAR(255),
\040\040contact_person\040VARCHAR(255),
\040\040contact_email\040VARCHAR(255),
\040\040company_website\040VARCHAR(255),
\040\040program_context\040TEXT,
\040\040component_types_needed\040TEXT,
\040\040estimated_volume\040VARCHAR(255),
\040\040target_regions\040VARCHAR(255),
\040\040quality_expectations\040TEXT,
\040\040special_requirements\040TEXT,
\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP
);
